package thread.basic;

public class RunnableTest02 implements Runnable {

	@Override
	public void run() {
		for(int i=1; i<=20; i++) {
			System.out.print(+i+"("+Thread.currentThread().getName()+") ");
			if(i%5 == 0) {
				System.out.println();
			}
			try {
				Thread.sleep(300);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
	}

	public static void main(String[] args) {
		

	}

}
