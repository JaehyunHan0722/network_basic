package basic;

import java.net.MalformedURLException;
import java.net.URL;

//URL클래스
//=> 인터넷에서 확인할 수 있는 자원을 추출
public class URLTest01 {

	public static void main(String[] args) {
		try {
			URL url = new URL("https://www.naver.com/");
		} catch (MalformedURLException e) {
			e.printStackTrace();
		}

	}

}
