package thread.basic;

class Super{
	
}
//이미 상속하고 있는 클래스가 있는 경우 Thread프로그래밍을 하기 우해서 Runnable
class RunnableDemo01 extends Super implements Runnable{

	@Override
	public void run() {
		for(int i=1; i<=20; i++) {
			System.out.print(+i+"("+") ");
			if(i%5 == 0) {
				System.out.println();
			}
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
		
	}
	
}

public class RunnableTest01 {

	public static void main(String[] args) {
		

	}

}
